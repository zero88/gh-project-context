{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "lib": [
      "es2022"
    ],
    "newLine": "lf",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./out",
    "rootDir": "./"
  },
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "../src"
    }
  ]
}
